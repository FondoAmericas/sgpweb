package pe.com.fondam.sgp.core.form.planOperativo;

public class PeriodoMontoForm {
	private String numeroPerido;
	private Double montoTotal;

	public String getNumeroPerido() {
		return numeroPerido;
	}

	public void setNumeroPerido(String numeroPerido) {
		this.numeroPerido = numeroPerido;
	}

	public Double getMontoTotal() {
		return montoTotal;
	}

	public void setMontoTotal(Double montoTotal) {
		this.montoTotal = montoTotal;
	}

}
