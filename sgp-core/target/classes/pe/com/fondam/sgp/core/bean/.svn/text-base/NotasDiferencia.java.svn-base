package pe.com.fondam.sgp.core.bean;

import java.io.Serializable;

public class NotasDiferencia implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -2394920243742675429L;
	private double nota01;
	private int nota01EvaluacionId;
	private double nota02;
	private int nota02EvaluacionId;
	private int diferencia;
	
	public NotasDiferencia() {
	}

	public double getNota01() {
		return nota01;
	}

	public void setNota01(double nota01) {
		this.nota01 = nota01;
	}

	public int getNota01EvaluacionId() {
		return nota01EvaluacionId;
	}

	public void setNota01EvaluacionId(int nota01EvaluacionId) {
		this.nota01EvaluacionId = nota01EvaluacionId;
	}

	public double getNota02() {
		return nota02;
	}

	public void setNota02(double nota02) {
		this.nota02 = nota02;
	}

	public int getNota02EvaluacionId() {
		return nota02EvaluacionId;
	}

	public void setNota02EvaluacionId(int nota02EvaluacionId) {
		this.nota02EvaluacionId = nota02EvaluacionId;
	}

	public int getDiferencia() {
		return diferencia;
	}

	public void setDiferencia(int diferencia) {
		this.diferencia = diferencia;
	}


}
